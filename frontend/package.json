{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "api:generate": "openapi-typescript-codegen --input http://localhost:8080/api/v1/swagger/doc.json --output ./src/shared/api --client axios",
    "api:generate:prod": "openapi-typescript-codegen --input https://afst-1.onrender.com/api/v1/swagger/doc.json --output ./src/shared/api --client axios",
    "api:generate:local": "openapi-typescript-codegen --input ../docs/swagger.json --output ./src/shared/api --client axios",
    "dev": "vite",
    "build": "npm run api:generate:prod && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-api-client": "npm run api:generate:local"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "epubjs": "^0.3.93",
    "lucide-react": "^0.555.0",
    "pdfjs-dist": "^5.4.449",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
